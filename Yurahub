local function CreateTPButton(name, pos)
    TPTab:CreateButton({
        Name = name,
        Callback = function()
            if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
                LP.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
            end
        end
    })
end

