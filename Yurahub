local SpamTab = Window:CreateTab("ãƒãƒ£ãƒƒãƒˆè’ã‚‰ã—", 4483362458)
SpamTab:CreateInput({Name = "é€£æŠ•ã™ã‚‹è¨€è‘‰", PlaceholderText = "å†…å®¹...", RemoveTextAfterFocusLost = false, Callback = function(v) SpamWord = v end})
SpamTab:CreateInput({Name = "å›žæ•°", PlaceholderText = "20", RemoveTextAfterFocusLost = false, Callback = function(v) SpamCount = tonumber(v) or 20 end})
SpamTab:CreateInput({Name = "ç§’é€Ÿ (é€ä¿¡é–“éš”)", PlaceholderText = "0.2", RemoveTextAfterFocusLost = false, Callback = function(v) SpamSpeed = tonumber(v) or 0.2 end})

local function sendMessage(msg)
    if TextChatService.ChatVersion == Enum.ChatVersion.TextChatService then
        local channel = TextChatService.TextChannels.RBXGeneral
        if channel then channel:SendAsync(msg) end
    else
        local remote = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
        if remote and remote:FindFirstChild("SayMessageRequest") then
            remote.SayMessageRequest:FireServer(msg, "All")
        end
    end
end

SpamTab:CreateButton({
    Name = "é€£æŠ•é–‹å§‹",
    Callback = function()
        if IsSpamming or SpamWord == "" then return end
        IsSpamming = true
        for i = 1, SpamCount do
            if not IsSpamming then break end
            sendMessage(SpamWord)
            task.wait(SpamSpeed)
        end
        IsSpamming = false
    end
})
SpamTab:CreateButton({Name = "ä¸­æ­¢ (STOP)", Callback = function() IsSpamming = false end})

local function Refresh()
    local names = {}
    for _, p in pairs(Players:GetPlayers()) do if p ~= LP then table.insert(names, p.Name) end end
    TrollDropdown:Refresh(names, true)
    SpyDropdown:Refresh(names, true)
    TPDropdown:Refresh(names, true)
end
Players.PlayerAdded:Connect(Refresh); Players.PlayerRemoving:Connect(Refresh); Refresh()

RunService.Heartbeat:Connect(function()
    pcall(function()
        local char = LP.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        local hum = char and char:FindFirstChild("Humanoid")
        if root and hum then
            if SpeedEnabled and hum.MoveDirection.Magnitude > 0 then
                root.Velocity = Vector3.new(hum.MoveDirection.X * SpeedValue, root.Velocity.Y, hum.MoveDirection.Z * SpeedValue)
            end
            if JumpEnabled then
                hum.UseJumpPower = true; hum.JumpPower = JumpValue
            elseif hum.JumpPower ~= 25 then
                hum.UseJumpPower = true; hum.JumpPower = 25
            end
            if Trolling and TrollTarget and TrollTarget.Character then
                local tRoot = TrollTarget.Character:FindFirstChild("HumanoidRootPart")
                if tRoot then
                    local offset = (TrollMode == "Front" and tRoot.CFrame.LookVector * TrollDistance) or (tRoot.CFrame.LookVector * -TrollDistance)
                    root.CFrame = tRoot.CFrame + offset
                end
            end
        end
        if FOVEnabled then Camera.FieldOfView = FOVValue end
    end)
end)

task.spawn(function()
    while true do
        if AutoClickEnabled then
            pcall(function()
                if ClickType == "Left" then
                    VU:Button1Down(Vector2.new(0, 0))
                    VU:Button1Up(Vector2.new(0, 0))
                else
                    VU:Button2Down(Vector2.new(0, 0))
                    VU:Button2Up(Vector2.new(0, 0))
                end
                if LP.Character then
                    local tool = LP.Character:FindFirstChildOfClass("Tool")
                    if tool then tool:Activate() end
                end
            end)
        end
        task.wait(1 / ClickCPS)
    end
end)

UIS.JumpRequest:Connect(function() if InfiniteJumpEnabled and LP.Character then LP.Character.Humanoid:ChangeState("Jumping") end end)
