local TPTab = Window:CreateTab("ãƒ†ãƒ¬ãƒãƒ¼ãƒˆ", 4483362458)

-- ä¸Šå´ã«ãƒ—ãƒ¬ã‚¤ãƒ¤ãƒ¼ãƒ†ãƒ¬ãƒãƒ¼ãƒˆ
TPTab:CreateSection("ðŸ“ ãƒ—ãƒ¬ã‚¤ãƒ¤ãƒ¼ã¸ç§»å‹•")
local TPDropdown = TPTab:CreateDropdown({
    Name = "ãƒªã‚¹ãƒˆã‹ã‚‰é¸æŠž",
    Options = {},
    Callback = function(Option)
        local targetName = type(Option) == "table" and Option[1] or Option
        TeleportTarget = Players:FindFirstChild(targetName)
    end,
})
TPTab:CreateButton({
    Name = "é¸æŠžã—ãŸäººã¸é£›ã¶",
    Callback = function()
        if TeleportTarget and TeleportTarget.Character and TeleportTarget.Character:FindFirstChild("HumanoidRootPart") then
            LP.Character.HumanoidRootPart.CFrame = TeleportTarget.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 3)
        end
    end
})
TPTab:CreateInput({
    Name = "åå‰æ¤œç´¢",
    PlaceholderText = "Username...",
    RemoveTextAfterFocusLost = false,
    Callback = function(v) TPNameInput = v:lower() end
})
TPTab:CreateButton({
    Name = "å…¥åŠ›ã—ãŸåå‰ã®äººã¸é£›ã¶",
    Callback = function()
        if TPNameInput == "" then return end
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LP and (string.find(p.Name:lower(), TPNameInput) or string.find(p.DisplayName:lower(), TPNameInput)) then
                if p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                    LP.Character.HumanoidRootPart.CFrame = p.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 3)
                    break
                end
            end
        end
    end
})

-- ä¸‹å´ã«åœ°ç‚¹ãƒ†ãƒ¬ãƒãƒ¼ãƒˆ
