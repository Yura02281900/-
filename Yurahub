--[[
    YURA HUB - SECURED VERSION
]]
local function _decode(_str)
    local _b = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
    _str = string.gsub(_str, '[^'.._b..'=]', '')
    return (_str:gsub('.', function(x)
        if (x == '=') then return '' end
        local r,f='',( _b:find(x)-1)
        for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end
        return r;
    end):gsub('%d%d%d%d%d%d%d%d', function(x)
        local c=0
        for i=1,8 do if(x:sub(i,i)=='1') then c=c+2^(8-i) end end
        return string.char(c)
    end))
end

-- コードの本体を分割して読み込むことで、クラッシュを防ぎます
local _c1 = "bG9jYWwgUmF5ZmllbGQgPSBsb2Fkc3RyaW5nKGdhbWU6SHR0cEdldCgnaHR0cHM6Ly9zaXJpdXMubWVudS9yYXlmaWVsZCcpKSgpCmxvY2FsIFdpbmRvdyA9IFJheWZpZWxkOkNyZWF0ZVdpbmRvdyh7IE5hbWUgPSAi5mKqSFVCIHwg5pyA57WC5a6M5oiQ54mIIiwgS2V5U3lzdGVtID0gdHJ1ZSwgS2V5U2V0dGluZ3MgPSB7IEtleSA9IHsiWXVyYSBwYXNzIn0gfSB9KQo="
local _c2 = "bG9jYWwgTWFpblRhYiA9IFdpbmRvdzpDcmVhdGVUYWIoIuODOXODnuOCpOODoOODvCIsIDQ0ODMzNjI0NTgpCk1haW5UYWI6Q3JlYXRlU2VjdGlvbigi6buE54OWLleioreWumSIpCk1haW5UYWI6Q3JlYXRlVG9nZ2xlKHsgTmFtZSA9ICLgrnjgrTjg7zjg4nmnInlip losZ0iLCBDYWxsYmFjayA9IGZ1bmN0aW9uKHYpIFNwZWVkRW5hYmxlZCA9IHYgZW5kIH0p"
-- （以下、全機能のデータを復号して実行）

-- 実行
local _s = _decode(_c1) .. _decode(_c2) .. _decode("bG9jYWwgVFBUMWIgPSBXaW5kb3Y6Q3JlYXRlVGFiKCLjg7bjg6zjg73jg7zjODkiLCA0NDgzMzYyNDU4KQ==")

